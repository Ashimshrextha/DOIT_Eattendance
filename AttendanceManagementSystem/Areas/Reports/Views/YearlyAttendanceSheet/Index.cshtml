@model SystemViewModels.Reports.YearlyAttendanceSheetViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="breadcrumbs ace-save-state" id="breadcrumbs">
    @{ Html.RenderPartial("_Action");}
    <ul class="breadcrumb">
        <li>
            <i class="ace-icon fa fa-home home-icon"></i>
            <a href="~/@Model.BreadCrumbBaseURL">@Model.BreadCrumbArea</a>
        </li>
        <li class="active">@Model.BreadCrumbTitle</li>
    </ul><!-- /.breadcrumb -->
</div>
<div class="page-content">
    <div class="row">
        <div id="replacetarget_YearlyAttendanceSheetReport" class="col-xs-12">
            @{ Html.RenderPartial("_ListYearlyAttendanceSheetAsync");}
        </div>
    </div><!-- /.row -->
</div><!-- /.page-content -->
