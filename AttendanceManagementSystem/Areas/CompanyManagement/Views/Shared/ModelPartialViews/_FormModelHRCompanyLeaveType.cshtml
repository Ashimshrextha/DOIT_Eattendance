        </div>
    </div>
</div>


<script type="text/javascript">
    $(document).ready(function () {
        var a = $('#DataModel_IdMasterLeaveTitle').val();
        debugger;
        if (a == '4' || a == '5' || a == '6' || a == '8' || a == '19' || a == '20' || a == '23') {
            $("#Recurring").show();
        }
        else {
            $("#Recurring").hide();
        }
    });
</script>


<script>
    $('#DataModel_IdMasterLeaveTitle').change(function () {
        var a = $('#DataModel_IdMasterLeaveTitle').val();
        debugger;
        if (a == '4' || a == '5' || a == '6' || a == '8' || a == '19' || a == '20' || a == '23') {
            $("#Recurring").show();
        }
        else {
            $("#Recurring").hide();
        }
    });
</script>
