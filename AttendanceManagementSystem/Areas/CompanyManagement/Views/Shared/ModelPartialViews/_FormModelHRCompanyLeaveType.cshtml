@model SystemViewModels.SystemSetting.HRCompanyLeaveTypeViewModel
<script src="~/Scripts/jquery.form.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
<script src="~/Scripts/RequiredField.js"></script>
@Html.AntiForgeryToken()
@Html.HiddenFor(model => model.DataModel.Id)
@Html.HiddenFor(model => model.PageNumber)
@Html.HiddenFor(model => model.PageSize)
@Html.HiddenFor(model => model.OrderingBy)
@Html.HiddenFor(model => model.OrderingDirection)
@Html.HiddenFor(model => model.SearchKey)

<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
    <div class="panel panel-default">
        <div class="panel-body">
            <div class="form-group row">
                <div class="col-md-6 col-sm-6 col-lg-6">
                    @Html.LabelFor(model => model.DataModel.IdHRCompany, new { @class = "control-label" })
                    @Html.DropDownListFor(model => model.DataModel.IdHRCompany, new SelectList(Model.DDLCompany, "Key", "Value"), new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.DataModel.IdHRCompany)
                </div>
                <div class="col-md-6 col-sm-6 col-lg-6">
                    @Html.LabelFor(model => model.DataModel.IdMasterLeaveTitle, new { @class = "control-label" })
                    @Html.DropDownListFor(model => model.DataModel.IdMasterLeaveTitle, new SelectList(Model.DDLMasterLeaveTitle, "Key", "Value"), new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.DataModel.IdMasterLeaveTitle)
                </div>
            </div>
            <div class="form-group row">
                <div class="col-md-4 col-sm-4 col-lg-4">
                    @Html.LabelFor(model => model.DataModel.SavingLimitDay, new { @class = "control-label" })
                    @Html.TextBoxFor(model => model.DataModel.SavingLimitDay, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.DataModel.SavingLimitDay)
                </div>
                <div class="col-md-4 col-sm-4 col-lg-4">
                    @Html.LabelFor(model => model.DataModel.TotalLeaveDay, new { @class = "control-label" })
                    @Html.TextBoxFor(model => model.DataModel.TotalLeaveDay, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.DataModel.TotalLeaveDay)
                </div>
                <div class="col-md-4 col-sm-4 col-lg-4" id="Recurring">
                    @Html.LabelFor(model => model.DataModel.OccuringTime, new { @class = "control-label" })
                    @Html.TextBoxFor(model => model.DataModel.OccuringTime, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.DataModel.OccuringTime)
                </div>
            </div>
            <div class="form-group row">
                <div class="col-md-4 col-sm-4 col-lg-4">
                    @Html.LabelFor(model => model.DataModel.AdditionalDays, new { @class = "control-label" })
                    @Html.TextBoxFor(model => model.DataModel.AdditionalDays, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.DataModel.AdditionalDays)
                </div>
                <div class="col-md-4 col-sm-4 col-lg-4">
                    @Html.LabelFor(model => model.DataModel.DocUploadDeadLineDays, new { @class = "control-label" })
                    @Html.TextBoxFor(model => model.DataModel.DocUploadDeadLineDays, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.DataModel.DocUploadDeadLineDays)
                </div>
                <div class="col-md-4 col-sm-4 col-lg-4">
                    @Html.LabelFor(model => model.DataModel.ApprovalDeadLineDays, new { @class = "control-label" })
                    @Html.TextBoxFor(model => model.DataModel.ApprovalDeadLineDays, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.DataModel.ApprovalDeadLineDays)
                </div>
            </div>
        </div>
    </div>
</div>
