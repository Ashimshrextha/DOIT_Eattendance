@model SystemViewModels.CompanyManagement.HRCompanyDivisionViewModel

<div id="printThis">
    <table style="width: 100%;" aria-describedby="gbox_grid-table" role="grid" id="example" class="table table-bordered table-hover table-responsive">
        <thead>
            <tr>
                <th>आईडी</th>
                <th>प्रमुख आईडी</th>
                <th>शाखाको प्रकार</th>
                <th>शाखाको नाम</th>
                <th>संक्षिप्त नाम</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.DBModelList)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.IdParent_HRCompanyDivision</td>
                    <td>@item.HRCompanyDivisionType.HRCompanyDivisionTypeTitle</td>
                    <td>@item.HRCompanyDivisionName</td>
                    <td>@item.HRCompanyDivisionShortName</td>
                </tr>
            }
        </tbody>
    </table>
</div>

<script src="~/Scripts/printThis.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#btnPrint').click(function () {
            $('#printThis').printThis();
        });
    });
</script>