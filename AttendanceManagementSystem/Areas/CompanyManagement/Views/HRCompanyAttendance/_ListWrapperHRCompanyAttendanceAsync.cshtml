@model SystemViewModels.CompanyManagement.HRCompanyAttendanceViewModellist
@using SystemStores.GlobalMethod;
@using SystemStores.ENUMData;

<div class="breadcrumbs ace-save-state">
    <div class="nav-search" id="nav-search">
        <form class="form-search">
            <span class="input-icon">
                <a class="nav-search-input" href="~/@Model.BreadCrumbBaseURL/_CreateHRCompanyAttendanceAsync?idHRCompany=@Model.IdHRCompany" data-ajax-model="">
                    <i class="ace-icon fa fa-plus  nav-search-icon">@SystemGlobalMethod.GetButtonText(EnumGlobal.SubmitButtonText.Create)</i>
                </a>
            </span>
        </form>
    </div><!-- /.nav-search -->
</div>
@{ Html.RenderPartial("_ListHRCompanyAttendanceAsync");}

<script type="text/javascript">
    $(document).ready(function () {
        $(function () {
        $.ajaxSetup({ cache: false });
            $('#Create_CompanyAttendance').click(function (e) {
            var url;
                url = '/@Model.BreadCrumbBaseURL' + '/_CreateHRCompanyAttendanceAsync?idHRCompany=' +'@Model.IdHRCompany';
            $('#myModalContent').load(url, function () {
                $('#myModal').modal({
                    keyboard: true
                }, 'show');
                bindAjaxForm(this);
            });
            return false;
        });
        });
    });
</script>




